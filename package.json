{
  "name": "eslint-config-brad",
  "version": "2.0.2",
  "private": true,
  "description": "Brad's ESlint config",
  "repository": "https://github.com/bradzacher/eslint-config-brad.git",
  "author": "Brad Zacher <brad.zacher@gmail.com>",
  "license": "MIT",
  "resolutions": {
    "eslint": "^6.7.1"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.5",
    "eslint-plugin-react": "^7.16.0",
    "eslint-restricted-globals": "^0.2.0"
  },
  "devDependencies": {
    "@types/eslint": "^6.1.3",
    "@types/mkdirp": "^0.5.2",
    "@types/node": "^12.12.14",
    "eslint-find-rules": "^3.4.0",
    "eslint-import-resolver-node": "^0.3.2",
    "husky": "^3.1.0",
    "json-schema-to-typescript": "^7.1.0",
    "mkdirp": "^0.5.1",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "scripts": {
    "build": "make build",
    "ci": "make ci",
    "clean": "rimraf -g ./dist/",
    "lint": "eslint ./src ./scripts ./types --ext .ts,.js --cache",
    "format-no-write": "prettier \"./**/*.{ts,json,md}\" --config=.prettierrc.js",
    "format": "yarn format-no-write --write",
    "format-check": "yarn format-no-write --list-different",
    "postinstall": "yarn patch-package",
    "prepush": "make ci",
    "regenerate-types": "make regenerate-types",
    "typecheck": "tsc -p ./tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush"
    }
  }
}
